<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      
      <th>Username</th>
      <th>Icon</th>

      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user %></td>
      <td><%= image_tag user.icon, :class => "usericon"  %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>